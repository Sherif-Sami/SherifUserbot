{
  "name": "SherifUserbot",
  "description": "A simple Telegram userbot based on Telethon . Maintained by Sherif Sami ",
  "logo": "https://telegra.ph/file/3d60313110c58684b31ea.jpg",
  "keywords": [
    "plugin",
    "modular",
    "productivity"
  ],
  "repository": "https://github.com/Sherif-Sami/SherifUserbot",
  "website": "#TODO",
  "success_url": "#TODO",
  "env": {
    "ALIVE_NAME": {
      "description": "give your name",
      "value": ""
    },
    "APP_ID": {
      "description": "Get this value from my.telegram.org",
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org",
      "value": ""
    },
    "STRING_SESSION": {
      "description": "Get this value from @SessionStringGeneratorZBot",
      "value": ""
    },
    "TG_BOT_TOKEN": {
      "description": "Needed for inline buttons maker. Make a bot at http://telegram.dog/BotFather and get the token of your bot.",
      "value": ""
    },
    "COMMAND_HAND_LER": {
      "description": "Set this one with only one the symbol to use it before your command to run like . , ' `",
      "value": ".",
      "required": false
    },
    "ENV": {
      "description": "Setting this to ANYTHING will enable heroku.",
      "value": "ANYTHING",
      "required": false
    },
    "HEROKU_API_KEY": {
      "description": "Get this value from https://dashboard.heroku.com/account",
      "value": "",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Your app name ",
      "value": "",
      "required": false
    },
    "TZ": {
      "description": "Get this value from http://www.timezoneconverter.com/cgi-bin/findzone.tzc",
      "value": "Africa/Cairo",
      "required": false
    },
    "EXTERNAL_REPO": {
      "description": "To install external plugins. if you don't want, change value to 'False'",
      "value": "True",
      "required": false
    },
    "BADCAT": {
      "description": "set 'True' to install 18+ plugins.",
      "value": "True",
      "required": false
    }
  },
  "addons": [{
    "plan": "heroku-postgresql",
    "options": {
      "version": "12"
    }
  }],
  "buildpacks": [{
    "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  },{
    "url":"https://github.com/heroku/heroku-buildpack-google-chrome"
  },{
    "url":"https://github.com/heroku/heroku-buildpack-chromedriver"
  },{
    "url": "https://github.com/riganf/heroku-buildpack-p7zip"
  },{
    "url": "https://github.com/heroku/heroku-buildpack-apt.git"
  },{
    "url": "https://github.com/chrismytton/heroku-buildpack-jq"
  },{
    "url": "https://github.com/rahulps1000/heroku-buildpack-nodejs"
  },{
    "url": "heroku/python"
  }],
  "stack": "heroku-22"
}
